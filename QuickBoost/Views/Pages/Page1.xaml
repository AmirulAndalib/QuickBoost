        <Button Content="Disable Preemption" HorizontalAlignment="Left" Margin="25,220,0,0" VerticalAlignment="Top" Click="DisablePreemption" Width="164"/>
        <Button Content="Enable Preemption" HorizontalAlignment="Left" Margin="25,256,0,0" VerticalAlignment="Top" Click="EnablePreemption" Width="164"/>
